{
  "name": "ascend-elevators",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "codegen": "graphql-codegen --config codegen.ts",
    "generate": "graphql-codegen --config codegen.ts",
    "generate:watch": "graphql-codegen --config codegen.ts --watch",
    "generate:dev": "NODE_TLS_REJECT_UNAUTHORIZED=0 graphql-codegen --config codegen.ts",
    "graphql:download": "curl -k -X POST https://localhost:5001/graphql -H \"Content-Type: application/json\" -d '{\"query\":\"{__schema{types{name fields{name type{name kind}}}}}\"}' > schema.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^21.0.3",
    "@angular/common": "^21.0.3",
    "@angular/compiler": "^21.0.3",
    "@angular/core": "^21.0.3",
    "@angular/forms": "^21.0.3",
    "@angular/platform-browser": "^21.0.3",
    "@angular/platform-browser-dynamic": "^21.0.3",
    "@angular/router": "^21.0.3",
    "@apollo/client": "^4.0.9",
    "@fortawesome/fontawesome-free": "^7.1.0",
    "@ngneat/tailwind": "^7.0.3",
    "aos": "^2.3.4",
    "apollo-angular": "^13.0.0",
    "emailjs-com": "^3.2.0",
    "graphql": "^16.12.0",
    "graphql-ws": "^6.0.6",
    "ngx-countup": "^13.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular/build": "^21.0.2",
    "@angular/cli": "^21.0.2",
    "@angular/compiler-cli": "^21.0.3",
    "@graphql-codegen/cli": "^6.1.0",
    "@graphql-codegen/introspection": "^5.0.0",
    "@graphql-codegen/typescript": "^5.0.6",
    "@graphql-codegen/typescript-apollo-angular": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^5.0.6",
    "@graphql-codegen/typescript-resolvers": "5.1.4",
    "@types/aos": "^3.0.7",
    "@types/jasmine": "~5.1.0",
    "autoprefixer": "^10.4.21",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.13",
    "typescript": "~5.9.3"
  }
